
import sys
import urllib
#import urllib2
from urllib.request import urlopen
url = 'http://musicbrainz.org/ws/2/artist/?query=artist:fred'
s = urlopen(url)
contents = s.read()
file = open("xmlimporttest.xml", 'wb')
file.write(contents)
file.close()

